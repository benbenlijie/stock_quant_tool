#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules:/workspace/frontend/stock-selection-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/geostitch.js" "$@"
else
  exec node  "$basedir/../../../../../../d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/geostitch.js" "$@"
fi
